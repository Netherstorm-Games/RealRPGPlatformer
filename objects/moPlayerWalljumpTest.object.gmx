        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Inputs

rightkey = keyboard_check(ord("D"));
leftkey = -keyboard_check(ord("A"));
jumpkey = keyboard_check_pressed(vk_space);

move = leftkey + rightkey;
hspd = move * movespeed;
if (vspd &lt; 10) vspd += grav;

if place_meeting (x,y+1,voGround)
{
    jumps = jumpsmax
} 

if (jumpkey) &amp;&amp; (jumps &gt; 0)
{
    jumps -= 1;
    vspd = jumpkey * -jumpspeed;
    audio_play_sound(jumpsfx,3,false)
}

if place_meeting(x-1, y, voGround) &amp;&amp; !place_meeting(x, y+1, voGround) &amp;&amp; leftwall = false &amp;&amp; vspd &gt; 0 {
vspd = 1.5
movespeed = 0
if jumpkey {
x+=1
movespeed = 4
vspd = -6
leftwall = true }}

if place_meeting(x+1, y, voGround) &amp;&amp; !place_meeting(x, y+1, voGround) &amp;&amp; rightwall = false &amp;&amp; vspd &gt; 0 {
vspd = 1.5
movespeed = 0
if jumpkey {
x-=1
movespeed = 4
vspd = -6
rightwall = true }}

if place_meeting(x-1, y, voGround) {
rightwall = false }

if place_meeting(x+1, y, voGround) {
leftwall = false }

if place_meeting(x, y+1, voGround) {
leftwall = false
rightwall = false  }

if !place_meeting(x-1,y,voGround) || !place_meeting(x+1,y,voGround) &amp;&amp; movespeed = 0 {
movespeed = 4 }

// H collision
if (place_meeting(x+hspd,y,voGround)) {
    while(!place_meeting(x+sign(hspd),y,voGround)) {
        x += sign(hspd);
    }
    hspd = 0;
}
x += hspd;

// V collsion
if (place_meeting(x,y+vspd,voGround)) {
    while(!place_meeting(x,y+sign(vspd),voGround)) {
        y += sign(vspd);
    }
    vspd = 0;
}
y += vspd;

//Animate
if (move!=0) image_xscale = move  * global.orng;

if move !=0 &amp;&amp; vspd = 0 &amp;&amp; place_meeting(x,y+1,voGround) {
sprite_index = msPlayerMove }

if vspd &lt; 0 &amp;&amp; !place_meeting(x,y+1,voGround) {
sprite_index = msPlayerJump }

if vspd &gt; 0 &amp;&amp; !place_meeting(x,y+1,voGround) {
sprite_index = msPlayerFall }

if move = 0 &amp;&amp; vspd = 0 &amp;&amp; place_meeting(x,y+1,voGround) {
sprite_index = msPlayerIdle }
</string>
          </argument>
